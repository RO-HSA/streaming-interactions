{
  "name": "streaming-interactions",
  "displayName": "Streaming interactions",
  "version": "0.2",
  "description": "Interact with people around the world about your favorite shows and movies through a float sidebar on the streaming page",
  "author": "Robert Santos <robertdahora@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.9.0",
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.11.0",
    "@supabase/supabase-js": "^2.45.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "framer-motion": "^11.3.19",
    "lucide-react": "^0.408.0",
    "plasmo": "0.88.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.1",
    "react-markdown": "^8.0.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@parcel/optimizer-data-url": "2.9.3",
    "@parcel/transformer-inline-string": "2.9.3",
    "@types/chrome": "0.0.258",
    "@types/hast": "^3.0.4",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "prop-types": "^15.8.1",
    "react-toastify": "^10.0.5",
    "supabase": "^1.187.8",
    "typescript": "5.3.3"
  },
  "manifest": {
    "web_accessible_resources": [
      {
        "resources": [
          "assets/images/profile-picture.svg",
          "assets/images/chevron-down.svg",
          "assets/images/chevron-up.svg",
          "assets/images/icon.svg",
          "assets/images/logo.svg",
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ],
    "permissions": [
      "tabs"
    ]
  }
}
